{
  "name": "octoboard",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@airbrake/browser": "^2.1.3",
    "@artsy/fresnel": "^1.3.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "boardgame.io": "^0.43.3",
    "classnames": "^2.2.6",
    "concurrently": "^6.0.0",
    "esm": "^3.2.25",
    "firebase": "^8.2.9",
    "firebase-admin": "^9.5.0",
    "i18next": "^19.9.1",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^3.6.4",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dom-confetti": "^0.2.0",
    "react-helmet-async": "^1.0.9",
    "react-i18next": "^11.8.8",
    "react-query": "^3.12.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-toastify": "^7.0.3",
    "remove-accents": "^0.4.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3"
  },
  "scripts": {
    "start": "concurrently -p \"{time} [{name}]\" -t \"HH:mm:ss.SSS\" -n \"client,server,firebase,storybook\" -c \"green.bold,blue.bold,yellow.bold,magenta.bold\" \"npm:start:client\" \"npm:start:server\" \"npm:start:firebase\" \"npm:start:storybook\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write .",
    "prettier-check": "pretty-quick --staged",
    "prettier-ci": "pretty-quick --check",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "start:server": "node -r esm src/server.js",
    "start:client": "react-scripts start",
    "start:firebase": "firebase emulators:start",
    "start:storybook": "start-storybook -p 9009 -s public --quiet",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^0.13.0",
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/addon-viewport": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/cli": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.20",
    "firebase-tools": "^9.5.0",
    "husky": "^5.1.3",
    "prettier": "^2.2.1",
    "storybook": "^6.1.20",
    "storybook-react-router": "^1.0.8"
  }
}
